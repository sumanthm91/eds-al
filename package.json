{
  "name": "@adobe/aem-boilerplate",
  "private": true,
  "version": "1.3.0",
  "description": "Starter project for Adobe Helix",
  "scripts": {
    "lint:js": "eslint . --ext js,jsx",
    "lint:css": "stylelint blocks/**/*.css templates/**/*.css styles/*.css react-app/**/*.css",
    "lint": "npm run lint:js && npm run lint:css",
    "start": "aem up --print-index",
    "postinstall": "node postinstall.js",
    "add": "plop",
    "start-react": "webpack serve --mode development",
    "build": "webpack --mode production",
    "analyze": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer ./stats.json dist",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/aem-boilerplate.git"
  },
  "author": "Adobe",
  "license": "Apache License 2.0",
  "bugs": {
    "url": "https://github.com/adobe/aem-boilerplate/issues"
  },
  "homepage": "https://github.com/adobe/aem-boilerplate#readme",
  "devDependencies": {
    "@adobe/aem-cli": "^16.1.3",
    "@babel/core": "7.24.0",
    "@babel/eslint-parser": "7.23.10",
    "@babel/preset-react": "^7.23.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "babel-loader": "^9.1.3",
    "chai": "5.1.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.9",
    "mini-css-extract-plugin": "^2.8.1",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "stylelint": "16.2.1",
    "stylelint-config-standard": "36.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.2",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.3"
  },
  "dependencies": {
    "@adobe/aem-boilerplate": "file:",
    "@adobe/magento-storefront-event-collector": "^1.7.1",
    "@adobe/magento-storefront-events-sdk": "^1.7.1",
    "@dropins/storefront-pdp": "0.3.0",
    "@dropins/tools": "0.26.0",
    "axios": "^1.7.4",
    "cleave.js": "^1.6.0",
    "graphql": "^16.8.1",
    "otp-timer": "^1.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-otp-input": "^3.1.1",
    "urql": "^4.0.6",
    "zustand": "^4.5.4"
  },
  "overrides": {
    "node-plop": {
      "inquirer": "9.3.5"
    }
  }
}
